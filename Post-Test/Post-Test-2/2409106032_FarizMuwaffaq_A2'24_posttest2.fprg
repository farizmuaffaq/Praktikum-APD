    <function name="Main" type="None" variable="">
        <parameters/>
        <body>
            <declare name="Nama" type="String" array="False" size=""/>
            <declare name="NIM, HargaBeras, DiskonMawar, TotalMawar, DiskonSania, TotalSania, DiskonMaknyus, TotalMaknyus" type="Integer" array="False" size=""/>
